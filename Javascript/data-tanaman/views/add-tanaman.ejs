<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <title>Add Tanaman</title>
  </head>
  <body>
    <div class="container">
      <div class="row mt-2">
        <div class="col-12 col-lg-8 text-center">
          <h1>Add Tanaman</h1>
        </div>
      </div>
      <div class="row mt-2">
        <div class="col-12 col-lg-8">
          <form action="/dashboard" method="post">
            <div class="mb-3">
              <label for="kode" class="form-label">Kode Tanaman</label>
              <input type="text" class="form-control" id="kode" name="kode">
            </div>
            <div class="mb-3">
              <label for="nama" class="form-label">Nama Tanaman</label>
              <input type="text" class="form-control" id="nama" name="nama">
            </div>
            <div class="mb-3">
              <label for="keterangan" class="form-label">Keterangan Tanaman</label>
              <input type="text" class="form-control" id="keterangan" name="keterangan">
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
            <a href="/dashboard" class="btn btn-success float-end">Kembali</a>
          </form>
        </div>
      </div>
    </div>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
  </body>
</html>
